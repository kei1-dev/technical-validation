{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "japaneast"
    },
    "rgLbName": {
      "value": "rg-lb-bastion"
    },
    "rgVmName": {
      "value": "rg-vm-bastion"
    },
    "rgHubName": {
      "value": "rg-hub"
    },
    "hubPeSubnetId": {
      "value": "/subscriptions/<SUBSCRIPTION_ID>/resourceGroups/rg-hub/providers/Microsoft.Network/virtualNetworks/vnet-hub/subnets/snet-pe"
    },
    "lbSubnetId": {
      "value": "/subscriptions/<SUBSCRIPTION_ID>/resourceGroups/<LB_RG_NAME>/providers/Microsoft.Network/virtualNetworks/<LB_VNET_NAME>/subnets/snet-lb"
