    "plsSubnetId": {
      "value": "/subscriptions/<SUBSCRIPTION_ID>/resourceGroups/<LB_RG_NAME>/providers/Microsoft.Network/virtualNetworks/<LB_VNET_NAME>/subnets/snet-pls"
